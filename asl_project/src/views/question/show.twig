{% extends 'layouts/_default.twig' %}

{% block main_content %}
    <div>
        <h1>{{ ques.question }}</h1>
        <p><strong>ID: </strong>{{ ques.id }}</p>
        <p><strong>Created At: </strong>{{ ques.createdAt }}</p>
        <p><strong>Updated Last: </strong>{{ ques.updatedAt }}</p>
        <p>
            <a href="/questions/{{ ques.id }}/delete">Delete</a>
        </p>
    </div>
{% endblock %}